{
  "name": "cashcarryshop/sizya",
  "version": "0.0.14",
  "description": "library for synchronization some platforms",
  "type": "library",
  "require": {
    "php": "^8.0",
    "cashcarryshop/synchronizer": "^1.1.0",
    "respect/validation": "^2.3.0",
    "guzzlehttp/guzzle": "^7.8",
    "symfony/validator": "^7.1.4",
    "symfony/expression-language": "^7.1.4",
    "symfony/intl": "7.1.5"
  },
  "require-dev": {
    "saggre/phpdocumentor-markdown": "^0.1.3",
    "phpunit/phpunit": "^11.3.6"
  },
  "license": "unlicense",
  "autoload": {
    "psr-4": {
      "cashcarryshop\\sizya\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "cashcarryshop\\sizya\\tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "cashcarryshop",
      "email": "cashcarryshop@yandex.ru"
    }
  ],
  "minimum-stability": "dev",
  "scripts": {
    "test": "php vendor/bin/phpunit --display-warnings --display-incomplete --display-errors"
  }
}
